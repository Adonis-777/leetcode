class Solution {
public:
    bool containsDuplicate(vector<int>& nums) {
        map<int, int> a;

        int n = nums.size();
        for(int i = 0; i < n; i++){
            a[nums[i]]++;
            if(a[nums[i]] > 1){
                return true;
            }
        }

        return false;
        
    }
};
